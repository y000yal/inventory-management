{
   "name": "g-sys/dynamic-inventory-management",
   "description": "a package for inventory management",
   "license": "MIT",
   "require-dev": {
      "phpunit/phpunit": "^9",
      "orchestra/testbench": "^6.0",
      "intervention/image": "^2.4"
   },
   "autoload": {
      "psr-4": {
         "GeniussystemsNp\\InventoryManagement\\": "src/",
         "GeniussystemsNp\\InventoryManagement\\Database\\Factories\\": "database/factories/"
      }
   },
   "autoload-dev": {
      "psr-4": {
         "GeniussystemsNp\\InventoryManagement\\Tests\\": "tests"
      }
   },
   "scripts": {
      "test": "vendor/bin/phpunit",
      "test-f": "vendor/bin/phpunit --filter"
   },
   "authors": [
      {
         "name": "yoyal.limbu",
         "email": "yoyal.limbu@gmail.com"
      }
   ],
   "minimum-stability": "dev",
   "extra": {
      "laravel": {
         "providers": [
            "GeniussystemsNp\\InventoryManagement\\InventoryManagementProvider.php"
         ],
         "aliases": {
            "ImageUpload": "GeniussystemsNp\\InventoryManagement\\Facades\\ImageUploadFacade"
         }
      }
   },
   "require": {
      "laravel/legacy-factories": "1.x-dev"
   }
}
